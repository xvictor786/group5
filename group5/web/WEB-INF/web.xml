<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getContactData</servlet-name>
        <servlet-class>com.servlet.getContactData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddContact</servlet-name>
        <servlet-class>com.servlet.AddContact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetContactDataByID</servlet-name>
        <servlet-class>com.servlet.GetContactDataByID</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateContactById</servlet-name>
        <servlet-class>com.servlet.UpdateContactById</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DelContactById</servlet-name>
        <servlet-class>com.servlet.DelContactById</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>com.servlet.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getContactData</servlet-name>
        <url-pattern>/getContactData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddContact</servlet-name>
        <url-pattern>/AddContact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetContactDataByID</servlet-name>
        <url-pattern>/GetContactDataByID</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateContactById</servlet-name>
        <url-pattern>/UpdateContactById</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DelContactById</servlet-name>
        <url-pattern>/DelContactById</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
